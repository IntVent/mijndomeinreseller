{
    "name": "intvent/mijndomeinreseller",
    "description": "A PHP composer package for MijnDomeinReseller",
    "keywords": [
        "intvent",
        "mijndomeinreseller"
    ],
    "homepage": "https://github.com/IntVent/mijndomeinreseller",
    "license": "MIT",
    "authors": [
        {
            "name": "IntVent BV",
            "email": "info@intvent.nl",
            "homepage": "https://intvent.nl"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "IntVent\\MijnDomeinReseller\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IntVent\\MijnDomeinReseller\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
